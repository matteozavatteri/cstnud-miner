Propositions {
  b s c a
}

TimePoints {
  (B! : b : )
  (M : )
  (C? : c : )
  (S! : s : )
  (Z : )
  (A? : a : )
}

ContingentLinks {
}

Constraints {
  (Z - Z <= 0 : )
  (C - Z <= 5 : )
  (B - Z <= 10 : )
  (S - Z <= 10 : )
  (A - Z <= 1 : )
  (M - Z <= 1 : !a)
  (Z - C <= -1 : )
  (Z - B <= -1 : )
  (Z - S <= -1 : )
  (Z - A <= -1 : )
  (Z - M <= -1 : !a)
}

